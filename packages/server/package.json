{
  "name": "server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "test": "dotenv -e .env.test -- ./bin/test",
    "test:unit": "ava spec",
    "start": "fastify start -l info dist/index.js -a 0.0.0.0",
    "build": "bin/copy-assets && ttsc",
    "dev": "nodemon --watch src -e ts,ejs --exec \"yarn build && yarn dev:start\"",
    "dev:start": "fastify start -l info -P dist/index.js -a 0.0.0.0 -p 4000",
    "prisma:generate": "pnpify prisma generate --schema=./src/infrastructure/prisma/schema.prisma",
    "prisma:migrate:dev": "pnpify prisma migrate dev --schema=./src/infrastructure/prisma/schema.prisma",
    "prisma:migrate:deploy": "pnpify prisma migrate deploy --schema=./src/infrastructure/prisma/schema.prisma",
    "prisma:studio": "pnpify prisma studio --schema=./src/infrastructure/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/passport": "^2.2.0",
    "@fastify/secure-session": "^5.3.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/view": "^7.3.0",
    "@mobily/ts-belt": "4.0.0-rc.2",
    "@yarnpkg/pnpify": "^4.0.0-rc.12",
    "autoinjection": "^0.0.6",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "envalid": "^7.3.1",
    "fastify": "4.5.3",
    "fastify-cli": "^4.3.0",
    "fastify-plugin": "^4.2.1",
    "fishery": "^2.2.2",
    "passport-google-oauth20": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "yup": "^0.32.11",
    "~": "link:./dist"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@prisma/client": "4.7.1",
    "@types/ejs": "^3",
    "@types/node": "^18.0.5",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/yup": "^0",
    "@yarnpkg/sdks": "^3.0.0-rc.12",
    "ava": "^4.3.1",
    "c8": "^7.11.3",
    "concurrently": "^7.0.0",
    "dotenv-cli": "^6.0.0",
    "nodemon": "^2.0.20",
    "prisma": "4.7.1",
    "ts-node": "^10.4.0",
    "tsx": "^3.12.1",
    "ttypescript": "^1.5.13",
    "typescript": "4.8.3"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  }
}
