{
  "name": "server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "scripts": {
    "test": "dotenv -e .env.test -- ./bin/test",
    "test:unit": "ava spec",
    "start": "yarn run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "yarn run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/index.js",
    "prisma:generate": "pnpify prisma generate --schema=./src/infrastructure/prisma/schema.prisma",
    "prisma:migrate:dev": "pnpify prisma migrate dev --schema=./src/infrastructure/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.1.0",
    "@fastify/cookie": "7.2.0",
    "@fastify/oauth2": "^5.0.0",
    "@fastify/sensible": "4.1.0",
    "@fastify/session": "^9.0.0",
    "@prisma/client": "4.3.1",
    "@yarnpkg/pnpify": "^4.0.0-rc.12",
    "fastify": "^4.5.3",
    "fastify-cli": "^4.3.0",
    "fastify-plugin": "^4.2.1",
    "fishery": "^2.2.2",
    "my-prisma": "workspace:*"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/node": "^18.0.5",
    "@yarnpkg/sdks": "^3.0.0-rc.12",
    "ava": "^4.3.1",
    "c8": "^7.11.3",
    "concurrently": "^7.0.0",
    "dotenv-cli": "^6.0.0",
    "prisma": "4.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.7.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
